{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "allowEmptyCatch": true,
    "no-cond-assign": 0,
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-redeclare": "warn",
    "no-undef": "error",
    "no-global-assign": "warn",
    "no-empty": "warn",
    "complexity": ["warn", 40],
    "max-statements": ["warn", 100]
  }
}
